<div>
    <div class="w3-content">
        <div class="w3-row">
            <div class="container">
                <div class="row">
                    <div class="col-12 col-sm-6 col-md-4 col-lg-3">
                        <div class="our-team">
                            <div class="picture">
                            <img class="img-fluid" src=>
                            </div>
                            <div class="team-content">
                            <h3 class="name">{{usertechs.username}}</h3>
                            <h4 class="title">{{usertechs.email}}</h4>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div id='user-repos'class="card">
        <div>
            <button class="name noselect" > <a href="/account/addnew"></a> ADD</button>
        </div>
        <div class="h3-titles">
            <div class="div-titles">user-repos</div>
        </div>
        {{!-- All  created  --}}
        {{#each usertech.teches as |tech|}}
            <div id='user-repos'class="card">
                <div class="h3-titles">
                    <div class="div-titles" id="techcarduseremail">{{tech.user.username}}</div>
                </div>
                <small class="card-category" id="techcarddate">{{tech.updatedAt}}</small>
                <h3 class="card-heading" id="techcardtitle">{{tech.title}}</h3>
                <p>
                <h3 class="card-heading" id="techcarddescription">Description</h3>
                {{tech.description}}
                </p>
                <p>
                <h3 class="card-heading">Examples</h3>
                {{tech.examples}}

                </p>
                <h3 class="card-heading" id="techcardlanguage">{{tech.language.language_name}}</h3>
            </div>
        {{/each}}   
   </div>
    
    <div id="user-add" class="card">
