<div class=" techcontainer">   
  {{#each category.teches as |tech|}}
    <div id='user-repos'class="card1">
        <div>
            <button class="name noselect" id="addtech" data-category="{{tech.category_id}}" data-tech="{{tech.id}}">Save</button>
        </div>
        <div class="h3-titles">
            <div class="div-titles" id="techcarduseremail">{{tech.user.username}}</div>
        </div>
        <p class="card-category" id="techcarddate">{{tech.updatedAt}}</p>
        <h3 class="card-heading" id="techcardtitle">{{tech.title}}</h3>
        <p>
        <h3 class="card-heading" id="techcarddescription">Description</h3>
          {{tech.description}}
        </p>
        <p>
        <h3 class="card-heading">Examples</h3>
          {{tech.examples}}
        </p>
        <h3 class="card-heading" id="techcardlanguage">{{tech.language.language_name}}</h3>
    </div>
  {{/each}}
</div>
<script src="/js/tech-save.js"></script>


