<div>
    <div class="w3-content">
        <div class="w3-row">
            <div class="container">
                <div class="row">
                    <div class="col-12 col-sm-6 col-md-4 col-lg-3">
                        <div class="our-team">
                            <div class="picture">
                            </div>
                            <div class="team-content">
                            <h3 class="name">{{usertechs.username}}</h3>
                            <h4 class="title">{{usertechs.email}}</h4>
                            </div>
                            <div>
                                <button class="name noselect" id="editprofile">Edit </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id='user-repos'class="card">
        <div class="h3-titles">
            <div class="div-titles">Created Posts</div>
        </div>
        {{!-- All  created  --}}
        <fieldset>
            <button name="submit" type="submit" id="addtech" data-submit="...Sending">Add New</button>
        </fieldset>
        {{#each usertechs.teches as |tech|}}
            <div id='user-repos'class="card1">
                <div class="h3-titles">
                    <div class="div-titles" id="techcarduseremail">{{tech.user.username}}</div>
                </div>
                <small class="card-category" id="techcarddate">{{tech.updatedAt}}</small>
                <h3 class="card-heading" id="techcardtitle">{{tech.title}}</h3>
                <p>
                <h3 class="card-heading" id="techcarddescription">Description</h3>
                {{tech.description}}
                </p>
                <p>
                <h3 class="card-heading">Examples</h3>
                {{tech.examples}}
                </p>
                <h3 class="card-heading" id="techcardlanguage">{{tech.language.language_name}}</h3>
            </div>
        {{/each}}   
    </div>
    <div id="user-add" class="card1">
    <div class="h3-titles">
        <div class="div-titles">Saved Posts</div>
    </div>
    {{!-- All  saved --}}
        {{#each usertechs.tech as |tech|}}
            <div id='user-repos'class="card">
                <div class="h3-titles">
                    <div class="div-titles" id="techcarduseremail">{{tech.user.username}}</div>
                </div>
                <small class="card-category" id="techcarddate">{{tech.updatedAt}}</small>
                <h3 class="card-heading" id="techcardtitle">{{tech.title}}</h3>
                <p>
                <h3 class="card-heading" id="techcarddescription">Description</h3>
                {{tech.description}}
                </p>
                <p>
                <h3 class="card-heading">Examples</h3>
                {{tech.examples}}
                </p>
                <h3 class="card-heading" id="techcardlanguage">{{tech.language.language_name}}</h3>
            </div>
        {{/each}} 
</div>
 <script src="/js/account-page.js"></script>